<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="kr.co.ezenac.tbja.sampledev.NotiActivity"
    android:orientation="vertical"
    >
    <Button
        android:id="@+id/btn_basic_noti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="basic noti"
        />
    <Button
        android:id="@+id/btn_basic_expand"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="expand noti"
        />
    <Button
        android:id="@+id/btn_basic_custom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="custom noti"
        />
</LinearLayout>
